    "eslint": "^7.19.0",
    "eslint-plugin-jsdoc": "^31.6.0",
    "jest": "^26.6.3",
    "jest-expect-message": "^1.0.2",
    "text-encoding": "^0.7.0",
    "ts-jest": "^26.5.4",
